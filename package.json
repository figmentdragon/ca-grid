  },
  "homepage": "https://github.com/figmentdragon/grid#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "bootstrap": "^5.1.3",
    "css": "^3.0.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "serve": "^13.0.2"
  },
  "dependencies": {
    "@popperjs/core": "^2.10.2",
