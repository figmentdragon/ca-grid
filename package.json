{
  "name": "cjmt-grid",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "build:css": "npm-run-all compile:css prefix:css",
    "server": "serve --listen 3000",
    "compile:css": "node-sass --include-path node_modules --output-style compressed --source-map true --source-map-contents true --precision 6 scss -o assets/css/",
    "prefix:css": "postcss --replace assets/css/starter.css --use autoprefixer --map",
    "start": "npm-run-all --parallel watch server"
  },
